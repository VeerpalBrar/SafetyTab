{
  "manifest_version": 2,
  "name": "SafetyTab",
  "version": "0.0.1",
  "default_locale": "en",
  "description": "Quickly change tabs to a page of your choosing.",
  "permissions": [
          "tabs", 
          "storage"
        ],
  "background": {
  "scripts": ["background.js"]
        }
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
  },
}
